  m_Script: {fileID: 11500000, guid: f50140316f9615d409a3666aa3f9096f, type: 3}
  m_Name: Tutorial-S-Raline
  m_EditorClassIdentifier: 
  sequenceName: Tutorial-S-Raline
  sequenceDescription: 
  dialogue:
  - speaker: System Narrator
    dialogue: "<i>Alert sound effect</i> \n\"TANTANGAN LITERASI DIGITAL: Gunakan
      Metode SIFT untuk verifikasi informasi!\""
    imagePotrait:
      potraitImage: {fileID: 21300014, guid: 358b5d9f23bc9413e87c9948acc67ee1, type: 3}
      imageLocation: 0
      clearImage: 0
  - speaker: Player
    dialogue: '<i>Sits down, gestures for calm </i>

      "Okay pause dulu! Sebelum
      kita gaskeun share atau apapun, kita harus STOP dan check internal reaction
      kita dulu'
    imagePotrait:
      potraitImage: {fileID: 21300000, guid: 278706dbc0ee8354b91ae5df81a3d56b, type: 3}
      imageLocation: 0
      clearImage: 0
  - speaker: System Narrator
    dialogue: '<i>Calm, instructional tone </i>"SIFT Step 1: STOP - Recognize your
      emotional triggers!"'
    imagePotrait:
      potraitImage: {fileID: 21300014, guid: 358b5d9f23bc9413e87c9948acc67ee1, type: 3}
      imageLocation: 0
      clearImage: 0
  - speaker: Player
    dialogue: '<i>Leans forward, maintaining eye contact </i>

      "Jadi gini,
      waktu lo liat info tentang ''bayaran 100-250k'' plus ''merchandise gratis'',
      apa yang lo rasain?"'
    imagePotrait:
      potraitImage: {fileID: 21300000, guid: 0314721316878614681f4a53d9342fac, type: 3}
      imageLocation: 0
      clearImage: 0
  - speaker: Kanaya
    dialogue: "<i>Fidgets with phone, looks down sheepishly </i>\r\nHonestly? Langsung
      excited banget! Kayak 'wah ini kesempatan bagus nih, sayang kalau dilewatin!'\r"
    imagePotrait:
      potraitImage: {fileID: 21300000, guid: f1820abf54ff7a3459a7d046adb0bff8, type: 3}
      imageLocation: 0
      clearImage: 0
  - speaker: Gavi
    dialogue: "<i>Nods knowingly, adjusts imaginary glasses</i>\r\nSame! Plus ada
      pressure juga karena 'pendaftaran tersisa 12 jam'. Jadi kayak FOMO gitu.\r"
    imagePotrait:
      potraitImage: {fileID: 21300000, guid: 71470c394e134c54d969226aa9b2574e, type: 3}
      imageLocation: 0
      clearImage: 0
  - speaker: Player
    dialogue: "<i>Stands up, paces slightly </i>\r\nNah itu dia! Scammer emang jago
      banget manipulasi emotions kita. Mereka tau kalau excitement + urgency = impulse
      decision.\r"
    imagePotrait:
      potraitImage: {fileID: 21300000, guid: 972f6b0694c006941ad425ea3e87a547, type: 3}
      imageLocation: 0
      clearImage: 0
  - speaker: Kanaya
    dialogue: "<i>Snaps fingers in realization</i> \r\nOhhh jadi kayak marketing
      psychology tapi yang jahat gitu ya?\r"
    imagePotrait:
      potraitImage: {fileID: 21300000, guid: 809b4d8f9b073ba46b215a648355e05f, type: 3}
      imageLocation: 0
      clearImage: 0
  - speaker: Player
    dialogue: "<i> Points finger gun at Kanaya </i>\r\nPersis! Makanya step pertama
      tuh always STOP. Give yourself time to think rationally, jangan langsung reaktif.\r"
    imagePotrait:
      potraitImage: {fileID: 21300000, guid: 972f6b0694c006941ad425ea3e87a547, type: 3}
      imageLocation: 0
      clearImage: 0
